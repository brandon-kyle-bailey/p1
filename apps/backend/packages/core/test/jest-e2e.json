{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/dtos": "<rootDir>/../libs/dtos/src",
    "@app/dtos/(.*)": "<rootDir>/../libs/dtos/src/$1",
    "@app/logging": "<rootDir>/../libs/logging/src",
    "@app/logging/(.*)": "<rootDir>/../libs/logging/src/$1"
  }
}